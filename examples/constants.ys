# Test all Arduino constants and pin modes

const int BUTTON = 2
const int LED = 13

function start() {
    pinMode(BUTTON, INPUT_PULLUP)
    pinMode(LED, OUTPUT)
}

function loop() {
    int reading = digitalRead(BUTTON)
    
    # Test all constants
    if (reading == HIGH) {
        digitalWrite(LED, LOW)
    }
    
    if (reading == LOW) {
        digitalWrite(LED, HIGH)
    }
}

