# Test all Arduino constants and pin modes

const BUTTON = 2
const LED = 13

function setup():
    pinMode(BUTTON, INPUT_PULLUP)
    pinMode(LED, OUTPUT)

function loop():
    var reading = digitalRead(BUTTON)
    
    # Test all constants
    if reading == HIGH:
        digitalWrite(LED, LOW)
    
    if reading == LOW:
        digitalWrite(LED, HIGH)
