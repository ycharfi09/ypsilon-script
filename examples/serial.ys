@main

# Serial Print Example
# Demonstrates Serial communication

config {
  board: arduino_uno,
  clock: 16MHz,
  uart: on,
  port: auto
}

const int SENSOR_PIN = 0

on start {
    print("Starting sensor monitoring...")
}

on loop {
    mut int value = analogRead(SENSOR_PIN)
    print(value)
    delay(500)
}

