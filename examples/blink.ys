@main

# Blink Example - Classic Arduino LED Blink
# This demonstrates basic pin control and timing

config {
  board: arduino_uno,
  clock: 16MHz,
  uart: on,
  port: auto
}

const int LED_PIN = 13

on start {
    pinMode(LED_PIN, OUTPUT)
}

on loop {
    digitalWrite(LED_PIN, HIGH)
    delay(1000)
    digitalWrite(LED_PIN, LOW)
    delay(1000)
}

